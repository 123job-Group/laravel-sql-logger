{
    "name": "mnabialek/laravel-sql-logger",
    "description": "Log SQL queries in Laravel/Lumen framework",
    "keywords": ["laravel", "sql", "log", "logger", "lumen", "sql logger", "log sql queries"],
    "license": "MIT",
    "authors": [
        {
            "name": "Marcin Nabia≈Çek",
            "email": "dev@nabialek.org"
        }
    ],
    "require": {
        "illuminate/support": "5.*"
    },
    "autoload": {
        "psr-4": {
            "Mnabialek\\LaravelSqlLogger\\": "src/"
        }
    },
    "autoload-dev": {
      "psr-4": {
        "Mnabialek\\LaravelSqlLogger\\Tests\\": "tests"
      }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~2.5",
        "phpunit/phpunit": "^6.5",
        "mockery/mockery": "^1.0",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mnabialek\\LaravelSqlLogger\\Providers\\ServiceProvider"
            ]
        }
    } 
}
